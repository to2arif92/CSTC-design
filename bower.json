{
  "name": "cstc",
  "version": "0.0.2",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "jquery": "^3.2.1",
    "animate.css": "^3.5.2",
    "font-awesome": "fontawesome#^4.7.0",
    "jquery-backstretch": "^2.1.16",
    "jquery-migrate": "jquery.migrate-newest#^3.0.0",
    "waypoints": "^4.0.1",
    "slick-carousel": "slick.js#^1.7.1",
    "wow": "^1.1.2"
  },
  "install": {
    "base": "src",
    "options": {
      "includePackageNameInInstallPath": false
    },
    "path": {
      "js": "js",
      "css": "css",
      "eot": "fonts",
      "otf": "fonts",
      "svg": "fonts",
      "ttf": "fonts",
      "woff": "fonts",
      "woff2": "fonts",
      "less": "less",
      "sass": "sass",
      "scss": "scss"
    },
    "sources": {
      "bootstrap": {
        "mapping": [
          {
            "bower-components/bootstrap/dist/css/bootstrap.css": "bootstrap.css"
          },
          {
            "bower-components/bootstrap/dist/css/bootstrap-theme.css": "bootstrap-theme.css"
          },
          {
            "bower-components/bootstrap/dist/js/bootstrap.js": "bootstrap.js"
          },
          {
            "bower-components/bootstrap/dist/css/bootstrap.css": "bootstrap.css"
          },
          {
            "bower-components/bootstrap/dist/fonts/**": ""
          }
        ]
      },
      "font-awesome": {
        "mapping": [
          {
            "bower-components/font-awesome/fonts/**": ""
          },
          {
            "bower-components/font-awesome/css/font-awesome.css": "font-awesome.css"
          }
        ]
      },
      "waypoints": {
        "mapping": [
          {
            "bower-components/waypoints/lib/jquery.waypoints.js": "jquery.waypoints.js"
          }
        ]
      }
    }
  }
}
